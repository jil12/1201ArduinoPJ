void setup()
{
  Serial.begin (9600);        //set up a serial connection with the computer

  pinMode(trigPin, OUTPUT);   //the trigger pin will output pulses of electricity
  pinMode(echoPin, INPUT);    //the echo pin will measure the duration of pulses coming back from the distance sensor

  pinMode(buzzerPin, OUTPUT);   //set the buzzer pin to output

  wolf.attach(9);            //use pin 9 to control the wolf 
  door.attach(3);  //use pin 3 to control the door 
  girl.attach(5);  //use pin 5 to control the girl 

  display.begin(SSD1306_SWITCHCAPVCC, 0x3C); //set oled
  display.display();
  display.clearDisplay();

}

void loop() {

  door.write(0);
  moveWolf(); //make the little wolf run

  display.clearDisplay();
  display.drawBitmap(32, 0, frame0, 64, 64, WHITE); //display default face
  display.display();

  distance = getDistance();   //variable to store the distance measured by the sensor

  Serial.print(distance);     //print the distance that was measured
  Serial.println(" in");      //print units after the distance

  if (1 < distance && distance <= 8) {                     //if little wolf is close
    roar(); //granny wolf roar
    delay(1000);
    door.write(180); //flip the door servo 180 degree. need find a way to make it stay
    shakeGirl(); //shake the bed
    emoji_animation();
  }
  else{
    door.write(0);
    display.clearDisplay();
    display.drawBitmap(32, 0, frame0, 64, 64, WHITE);
    display.display();
  }

  delay(50);      //delay 50ms between each reading
}

